{
  "name": "Complete Information",
  "description": "Comprehensive overlay with timelapse name, weather, timestamp, and frame count",
  "overlay_config": {
    "overlayPositions": {
      "topLeft": {
        "type": "timelapse_name",
        "textSize": 20,
        "textColor": "#FFFFFF",
        "backgroundOpacity": 70,
        "enableBackground": true,
        "backgroundColor": "#1F2937"
      },
      "topRight": {
        "type": "weather_temp_conditions",
        "textSize": 14,
        "textColor": "#FFFFFF",
        "backgroundOpacity": 50,
        "enableBackground": true,
        "backgroundColor": "#1E3A8A",
        "unit": "fahrenheit",
        "display": "both"
      },
      "bottomLeft": {
        "type": "date_time",
        "textSize": 16,
        "textColor": "#FFFFFF",
        "backgroundOpacity": 60,
        "dateFormat": "MM/dd/yyyy HH:mm:ss",
        "enableBackground": true,
        "backgroundColor": "#000000"
      },
      "bottomRight": {
        "type": "frame_number",
        "textSize": 14,
        "textColor": "#FFFFFF",
        "backgroundOpacity": 40,
        "enableBackground": true,
        "backgroundColor": "#374151",
        "leadingZeros": 6,
        "hidePrefix": false
      }
    },
    "globalOptions": {
      "opacity": 100,
      "font": "Arial",
      "xMargin": 25,
      "yMargin": 25,
      "backgroundColor": "#000000",
      "backgroundOpacity": 55,
      "fillColor": "#FFFFFF",
      "dropShadow": 3,
      "preset": "complete_info"
    }
  },
  "is_builtin": true
}